<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="javaCollections" class = "com.springtutorial.JavaCollections">
	<property name = "addressList">
		<list>
			<value>India</value>
			<value>Sri Lanka</value>
			<value>US</value>
			<value>US</value>		
		</list>
	</property>
	
	<property name = "addressSet">
		<set>
			<value>India</value>
			<value>Sri Lanka</value>
			<value>US</value>
			<value>US</value>
		</set>
	</property>
	
	<property name = "addressMap">
		<map>
			<entry key="1" value ="India"/>	
			<entry key="2" value ="Sri Lanka"/>	
			<entry key="3" value ="US"/>	
			<entry key="4" value ="US"/>	
		</map>
	</property>
	
	<property name = "addressProp">
		<props>
				<prop key="one">India</prop>
				<prop key="two">Sri Lanka</prop>
				<prop key="three">US</prop>
				<prop key="four">US</prop>
		</props>
	</property>
</bean>

</beans>